[WINDOW]
QuickWindow WINDOW('Form Triggers'),AT(,,449,183),FONT('Segoe UI',10,,FONT:regular,CHARSET:ANSI), |
          RESIZE,AUTO,CENTER,GRAY,IMM,MAX,HLP('UpdateTrigger'),SYSTEM,WALLPAPER('Background1.png')
          SHEET,AT(2,1,446,180),USE(?CurrentTab),ABOVE,#ORIG(CurrentTab),#ORDINAL(1)
            TAB('General'),USE(?GeneralTab),#ORDINAL(2)
              PROMPT('Parent GUID:'),AT(7,17),USE(?Tri:ParentGUID:Prompt),TRN,#LINK(?Tri:ParentGUID), |
                #ORIG(?Tri:ParentGUID:Prompt),#ORDINAL(3)
              ENTRY(@s16),AT(101,17,68,10),USE(Tri:ParentGUID),LEFT(2),#LINK(?Tri:ParentGUID:Prompt), |
                #ORIG(Tri:ParentGUID),#ORDINAL(4)
              PROMPT('Guid:'),AT(7,29),USE(?Tri:Guid:Prompt),TRN,#LINK(?Tri:Guid),#ORIG(?Tri:Guid:Prompt), |
                #ORDINAL(5)
              ENTRY(@s40),AT(101,29,164,10),USE(Tri:Guid),LEFT(2),#LINK(?Tri:Guid:Prompt), |
                #ORIG(Tri:Guid),#ORDINAL(6)
              PROMPT('Trigger Type:'),AT(7,44),USE(?Tri:TriggerType:Prompt),TRN,#LINK(?Tri:TriggerType), |
                #ORIG(?Tri:TriggerType:Prompt),#ORDINAL(7)
              ENTRY(@s20),AT(101,44,84,10),USE(Tri:TriggerType),LEFT(2),#LINK(?Tri:TriggerType:Prompt), |
                #ORIG(Tri:TriggerType),#ORDINAL(8)
              TEXT,AT(7,57,431,116),USE(?CodeText),FONT('Courier New',10),VSCROLL,#ORDINAL(9)
            END
            TAB('Comments'),USE(?CommentsTab),#ORDINAL(10)
              LIST,AT(7,38,432,113),USE(?CommentsList),FORMAT('44L(2)M~Create Date~@s11' & |
                '@#5#108L(2)M~Create User~@s50@#8#1020L(2)M~Text~@s255@#2#'),#ORDINAL(11)
              BUTTON,AT(31,18,18,15),USE(?EditCommentBtn),ICON('Edit3.ico'),DISABLE,FLAT,#ORDINAL(12)
              BUTTON,AT(50,18,18,15),USE(?DeleteCommentBtn),ICON('GarbageClosed.ico'),DISABLE, |
                FLAT,#ORDINAL(13)
              BUTTON,AT(10,18,18,15),USE(?AddCommentBtn),ICON('AddNew.ico'),FLAT,#ORDINAL(14)
            END
          END
          BUTTON,AT(397,1,23,12),USE(?OK),LEFT,ICON('Save1.ico'),DEFAULT,DISABLE,FLAT,HIDE, |
            MSG('Accept data and close the window'),TIP('Accept data and close the window'), |
            #ORIG(?OK),#SEQ(2),#ORDINAL(15)
          BUTTON,AT(423,1,23,12),USE(?Cancel),LEFT,ICON('Close.ico'),FLAT,HIDE,MSG('Cancel operation'), |
            TIP('Cancel operation'),#ORIG(?Cancel),#SEQ(3),#ORDINAL(16)
        END

