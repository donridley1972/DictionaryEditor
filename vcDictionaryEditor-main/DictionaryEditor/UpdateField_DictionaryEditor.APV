local.SaveGlyphBitmapToFile   PROCEDURE(STRING pFileName, *GLYPHMETRICS pGM, LONG pBuffer, LONG bufSize)
!bmh        LIKE(BITMAPFILEHEADER)
!bih        LIKE(BITMAPINFOHEADER)
