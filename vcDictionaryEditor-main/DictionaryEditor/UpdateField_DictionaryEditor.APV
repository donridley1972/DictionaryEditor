        Add(ControlTypesQ)
    End

    ControlTypesQ.CtrlType = 'STRING'
    ControlTypesQ.RowID = Records(ControlTypesQ)+1
    Add(ControlTypesQ)

    ControlTypesQ.CtrlType = 'TEXT'
    ControlTypesQ.RowID = Records(ControlTypesQ)+1
    Add(ControlTypesQ)


